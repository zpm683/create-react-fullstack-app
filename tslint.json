{
  "rules": {
    "max-line-length": [
      true,
      120
    ],
    "object-shorthand-properties-first": false,
    "radix": false,
    "ter-arrow-parens": false,
    "import-name": [
      false
    ],
    "variable-name": [
      true,
      "allow-pascal-case"
    ],
    "quotemark": [true, "double"],
    "trailing-comma": [false],
    "triple-equals": false,
    "prefer-array-literal":false,
    "no-else-after-return":false,
    "function-name":false,
    "no-implicit-dependencies": false
  }
}
